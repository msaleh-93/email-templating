                                                        <td style="padding: 0;">
                                                          <p
                                                            style="margin: 0; font-size: 14px; text-align: end; color: #003180;">
                                                            <strong>{{arrival.date}}</strong> {{arrival.time}}
                                                          </p>
                                                        </td>
                                                      </tr>
                                                    </tbody>
